<?xml version="1.0"?>
<root BTCPP_format="4" main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence name="MissionSequence">
      <Action ID="NavigateAction" waypoint="field_entry"/>
      <RetryUntilSuccessful num_attempts="3">
        <Sequence name="ExplorationSequence">
          <Action ID="ExploreFieldAction"/>
          <Condition ID="DetectTreeCondition"/>
        </Sequence>
      </RetryUntilSuccessful>
      <Action ID="TakePhotoAction" target="canopy_snapshot"/>
    </Sequence>
  </BehaviorTree>
</root>
